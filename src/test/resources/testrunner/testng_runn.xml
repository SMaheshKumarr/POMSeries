<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="TestNG Regression Suite" verbose="4"
	thread-count="10" parallel="tests">

<listeners>
<listener class-name="com.qa.listeners.ExtentReportListener"/>
<listener class-name="com.qa.listeners.TestAllureListener"/>
<listener class-name="com.qa.listeners.AnnotationTransformer"/>
</listeners>
	<test name="Login Page Test">
		<classes>
			<class name="com.qa.tests.LoginPageTest" />
		</classes>
	</test>


	<test name="Account Page Test">
		<classes>
			<class name="com.qa.tests.AccountPageTest"></class>
		</classes>
	</test>
	
	<test name="Product Info Page Test">
		<classes>
			<class name="com.qa.tests.ProductInfoPageTest"></class>
		</classes>
	</test>
	




</suite>